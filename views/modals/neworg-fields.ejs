<form id="neworg_form">
    <input type="text" name="table"  value="<%= table %>" hidden>
    <input type="text" name="localid" value = "<%= localid %>" hidden>

    <p><label>Nombre<!--Name--></label>
        <input name="name" type="text" class="form-control" required></p>
    <hr>
    <h4>Identificador</h4>
    <p><label>Esquema <!--Identifier scheme--></label>
        <input name="identifier_scheme" type="text" class="form-control" ></p>
    <p><label>ID <!--Identifier id--></label>
        <input name="identifier_id" type="text" class="form-control"></p>
    <p><label>Nombre legal<!--Identifier legal name--></label>
        <input name="identifier_legalname" type="text" class="form-control"></p>
    <p><label>URI<!--Identifier uri--></label>
        <input name="identifier_uri" type="text" class="form-control"></p>
    <hr>
    <h4>Dirección</h4>
    <p><label>Calle y número<!--Address street address--></label>
        <input name="address_streetaddress" type="text" class="form-control"></p>
    <p><label>Localidad<!--Address locality--></label>
        <input name="address_locality" type="text" class="form-control"></p>
    <p><label>Región<!--Address region--></label>
        <input name="address_region" type="text" class="form-control"></p>
    <p><label>Código postal <!--Address postal code--></label>
        <input name="address_postalcode" type="text" class="form-control"></p>
    <p><label>País<!--Address country name--></label>
        <input name="address_countryname" type="text" class="form-control"></p>
    <hr>
    <h4>Contacto</h4>
    <p><label>Nombre<!--Contact point name--></label>
        <input name="contactpoint_name" type="text" class="form-control"></p>
    <p><label>E-mail <!--Contact point email--></label>
        <input name="contactpoint_email"   type="text" class="form-control"></p>
    <p><label>Teléfono<!--Contact point telephone--></label>
        <input name="contactpoint_telephone"  type="text" class="form-control"></p>
    <p><label>Fax<!--Contact point fax number--></label>
        <input name="contactpoint_faxnumber"    type="text" class="form-control"></p>
    <p><label>URL <!--Contact point URL--></label>
        <input name="contactpoint_url"    type="text" class="form-control"></p>
    <button type="submit" class="btn btn-default">Crear</button>
</form>